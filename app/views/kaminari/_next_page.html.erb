<% if current_page.last? %>
  <li class="disabled">
    <%= content_tag :a do %>
      <i class="material-icons"><%= t "views.kaminari.next" %></i>
    <% end %>
  </li>
<% else %>
  <li class="waves-effect">
    <%= link_to url, :rel => 'next', :remote => remote, method: :post do %>
      <i class="material-icons"><%= t "views.kaminari.next" %></i>
    <% end %>
  </li>
<% end %>
